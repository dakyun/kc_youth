<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kc.thanks.chap.mapper.NoteEmailMapper">
    <insert id="saveNoteEmail" parameterType="com.kc.thanks.chap.dto.requestDTO.NoteEmailRequestDTO">
        INSERT INTO note_email (
        note_seq, email, use_yn, reg_dt, chg_dt
        )
        VALUES (
        #{note_seq}, #{email}, #{use_yn}, #{reg_dt}, #{chg_dt}
        )
    </insert>

</mapper>